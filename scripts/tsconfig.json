{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["esnext"],
    "alwaysStrict": true,
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": false,
    "newLine": "LF",
    "noEmitOnError": true,
    "removeComments": true
  },
  "ts-node": {
    "esm": true
  }
}
