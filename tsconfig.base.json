{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": false,
    "lib": [
      "ESNext"
    ],
    "module": "ESNext",
    "moduleResolution": "node",
    "newLine": "LF",
    "noEmitOnError": true,
    "resolveJsonModule": true,
    "strict": true,
    "target": "ES2018",
  }
}
