    "rollup": "^2.34.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "semantic-release": "^17.3.0",
    "shelljs": "^0.8.3",
    "ts-node": "^9.1.1",
    "tsc-prog": "^2.2.1",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3",
    "word-wrap": "^1.2.3"
  },
  "peerDependencies": {
    "eslint": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "typescript": "^3.4.1 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=10.18.0"
  }
}
