{
  "name": "eslint-plugin-functional",
  "version": "3.2.1",
  "description": "ESLint rules to disable mutation and promote fp in TypeScript.",
  "keywords": [
    "eslint",
    "eslint plugin",
    "fp",
    "functional",
    "functional programming",
    "immutability",
    "immutable"
  ],
  "homepage": "https://github.com/jonaskello/eslint-plugin-functional#readme",
  "bugs": {
    "url": "https://github.com/jonaskello/eslint-plugin-functional/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonaskello/eslint-plugin-functional"
  },
  "license": "MIT",
  "author": "Jonas Kello",
  "contributors": [
    {
      "name": "Rebecca Stevens",
      "email": "rebecca.stevens@outlook.co.nz"
    }
  ],
  "exports": {
    "default": "./lib/index.js",
    "import": "./lib/index.mjs",
    "require": "./lib/index.js"
  },
  "main": "lib/index.js",
  "files": [
    "lib/",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "yarn compile",
    "prebuild-tests": "rimraf build",
    "build-tests": "yarn compile-tests",
    "compile": "rollup -c",
    "compile-tests": "ts-node -P scripts/tsconfig.json scripts/compile-tests.ts",
    "prelint": "yarn build",
    "lint": "eslint .",
    "lint-fix": "yarn lint --fix",
    "test": "nyc ava",
    "test-work": "ava 'tests/rules/_work.test.ts'",
    "verify": "yarn lint && yarn test"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^4.9.1",
    "array.prototype.flatmap": "^1.2.4",
    "deepmerge": "^4.2.2",
    "escape-string-regexp": "^4.0.0",
    "object.fromentries": "^2.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@rebeccastevens/eslint-config": "^1.0.5",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.9",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/dedent": "^0.7.0",
    "@types/eslint": "^7.2.6",
    "@types/estree": "^0.0.45",
    "@types/glob": "^7.1.1",
    "@types/node": "14.14.12",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "ava": "^3.15.0",
    "babel-eslint": "^10.0.2",
    "chalk": "^4.1.0",
    "codecov": "^3.8.1",
    "commitizen": "^4.2.2",
    "conventional-commit-types": "^3.0.0",
    "cross-env": "^7.0.3",
    "cz-adapter-eslint": "^0.3.0",
    "dedent": "^0.7.0",
    "eslint": "^7.17.0",
    "eslint-ava-rule-tester": "^4.0.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-ava": "^11.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-eslint-plugin": "^2.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^25.0.1",
